!!! 5
%html
  %head
    = csrf_meta_tags
    = stylesheet_link_tag    'admin', :media => 'all'
    = javascript_include_tag 'admin'
    = javascript_include_tag 'rails'
  %body
    .wrapper
      .navbar.navbar-fixed-top
        .navbar-inner
          .container
            %a.brand{href: admin_root_path}
              Eromassage CRM
            %ul.nav
              %li.divider-vertical
              %li= link_to 'Девушки', admin_girls_path
              %li.divider-vertical
              %li= link_to 'Комнаты', admin_rooms_path
              %li.divider-vertical
              %li= link_to 'Программы', admin_programs_path
      .container#main
        .row
          .span12
            = yield
            = yield :scripts
      .push
